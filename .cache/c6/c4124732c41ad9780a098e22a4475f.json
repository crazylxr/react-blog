mn":32}},{"name":"renderLink","generated":{"line":74,"column":14},"source":"../src/views/web/article/navigation.jsx","original":{"line":49,"column":41}},{"generated":{"line":74,"column":24},"source":"../src/views/web/article/navigation.jsx","original":{"line":49,"column":32}},{"generated":{"line":74,"column":25},"source":"../src/views/web/article/navigation.jsx","original":{"line":49,"column":9}},{"generated":{"line":74,"column":26},"source":"../src/views/web/article/navigation.jsx","original":{"line":49,"column":2}},{"generated":{"line":75,"column":0},"source":"../src/views/web/article/navigation.jsx","original":{"line":50,"column":1}},{"generated":{"line":75,"column":1},"source":"../src/views/web/article/navigation.jsx","original":{"line":40,"column":0}},{"name":"Navigation","generated":{"line":77,"column":15},"source":"../src/views/web/article/navigation.jsx","original":{"line":52,"column":15}}],"sources":{"../src/views/web/article/navigation.jsx":"import React from 'react'\nimport { Anchor } from 'antd'\nconst { Link } = Anchor\n\n  // 转化为锚地可跳转的路径\nfunction transferHref(str) {\n  let v1 = str.replace(/(\\s)|(,)/g, '-')\n  let v2 = v1.replace(/[./()]/g, '')\n  return `#${v2.replace(/-+/g, '-')}`\n}\n\n// 根据 article 来生成锚点列表\nfunction getAnchorList(str) {\n  const pattern = /<(h[1-6])[\\s\\S]+?(?=<\\/\\1>)/g\n  let list = []\n  function pushItem(arr, item) {\n    const len = arr.length\n    const matchItem = arr[len - 1]\n    if (matchItem && matchItem.tag !== item.tag) {\n      pushItem(matchItem.children, item)\n    } else {\n      arr.push(item)\n      // debugger\n    }\n  }\n  str.replace(pattern, ($0, $1) => {\n    const title = $0.replace(/.*?>/, '')\n    const href = transferHref(title)\n    const currentItem = {\n      tag: $1, // 标签类型\n      title,\n      href,\n      children: []\n    }\n    pushItem(list, currentItem)\n  })\n  return list\n}\n\nconst Navigation = ({ content }) => {\n  const list = getAnchorList(content)\n  function renderLink({ href, title, children }) {\n    return (\n      <Link key={href} href={href} title={title}>\n        {children.length > 0 && children.map(sub => renderLink(sub))}\n      </Link>\n    )\n  }\n  return <Anchor affix={false}>{list.map(renderLink)}</Anchor>\n}\n\nexport default Navigation\n"},"lineCount":null}},"error":null,"hash":"a8802a4cbba76aa3fa70bc2618de3625","cacheData":{"env":{}}}