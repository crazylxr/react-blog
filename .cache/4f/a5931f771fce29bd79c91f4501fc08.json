<Menu.ItemGroup title=\"设置中心\" className=\"menu-group\">\n      <Menu.Item>个人设置</Menu.Item>\n      <Menu.Item>系统设置</Menu.Item>\n    </Menu.ItemGroup>\n  </Menu>\n)\n\n@connect(\n  null,\n  { logout }\n)\n@withRouter\nclass AdminHeader extends Component {\n  static propTypes = {\n    collapsed: PropTypes.bool.isRequired,\n    onToggle: PropTypes.func\n  }\n\n  handleLogout = () => {\n    console.log(this.props)\n    this.props.history.push('/login')\n  }\n\n  renderDropDownMenu = () => {\n    return (\n      <Menu className=\"menu\">\n        <Menu.Item>\n          <span onClick={() => this.props.history.push('/')}>返回主页</span>\n        </Menu.Item>\n        <Menu.Item>\n          <span onClick={this.handleLogout}>退出登录</span>\n        </Menu.Item>\n      </Menu>\n    )\n  }\n\n  render() {\n    const { collapsed } = this.props\n    return (\n      <div className=\"admin-header-container\">\n        <Icon\n          type={collapsed ? 'menu-unfold' : 'menu-fold'}\n          className=\"trigger\"\n          onClick={this.props.onToggle}\n        />\n        <div className=\"header-right\">\n          <Dropdown overlay={this.renderDropDownMenu()}>\n            <span>\n              <AuthorAvatar size=\"large\" />\n            </span>\n          </Dropdown>\n        </div>\n      </div>\n    )\n  }\n}\n\nexport default AdminHeader\n"},"lineCount":null}},"error":null,"hash":"2460d83765ba5b446c8a64667dc86f80","cacheData":{"env":{}}}