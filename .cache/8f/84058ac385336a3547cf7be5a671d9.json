"original":{"line":41,"column":3}},{"generated":{"line":66,"column":0},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":2}},{"generated":{"line":66,"column":9},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":9}},{"generated":{"line":67,"column":0},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":17}},{"generated":{"line":67,"column":4},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":17}},{"generated":{"line":67,"column":9},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":22}},{"generated":{"line":67,"column":11},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":24}},{"generated":{"line":68,"column":0},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":9}},{"name":"list","generated":{"line":68,"column":5},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":32}},{"generated":{"line":68,"column":9},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":36}},{"name":"map","generated":{"line":68,"column":10},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":37}},{"generated":{"line":68,"column":13},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":32}},{"name":"renderLink","generated":{"line":68,"column":14},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":41}},{"generated":{"line":68,"column":24},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":32}},{"generated":{"line":68,"column":25},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":9}},{"generated":{"line":68,"column":26},"source":"../src/examples/markdown/navigation.jsx","original":{"line":42,"column":2}},{"generated":{"line":69,"column":0},"source":"../src/examples/markdown/navigation.jsx","original":{"line":43,"column":1}},{"generated":{"line":69,"column":1},"source":"../src/examples/markdown/navigation.jsx","original":{"line":33,"column":0}},{"name":"Navigation","generated":{"line":71,"column":15},"source":"../src/examples/markdown/navigation.jsx","original":{"line":45,"column":15}}],"sources":{"../src/examples/markdown/navigation.jsx":"import React from 'react'\nimport { Anchor } from 'antd'\nconst { Link } = Anchor\n\n// 根据 article 来生成锚点列表\nfunction getAnchorList(str) {\n  const pattern = /<(h[1-6])[\\s\\S]+?(?=<\\/\\1>)/g\n  let list = []\n  function pushItem(arr, item) {\n    const len = arr.length\n    const matchItem = arr[len - 1]\n    if (matchItem && matchItem.tag !== item.tag) {\n      pushItem(matchItem.children, item)\n    } else {\n      arr.push(item)\n      // debugger\n    }\n  }\n  str.replace(pattern, ($0, $1) => {\n    const title = $0.replace(/.*?>/, '')\n    const href = `#${title.replace(/\\s/g, '-')}` // 转化为锚地可跳转的路径\n    const currentItem = {\n      tag: $1, // 标签类型\n      title,\n      href,\n      children: []\n    }\n    pushItem(list, currentItem)\n  })\n  return list\n}\n\nconst Navigation = ({ article }) => {\n  const list = getAnchorList(article)\n  function renderLink({ href, title, children }) {\n    return (\n      <Link key={href} href={href} title={title}>\n        {children.length > 0 && children.map(sub => renderLink(sub))}\n      </Link>\n    )\n  }\n  return <Anchor affix={false}>{list.map(renderLink)}</Anchor>\n}\n\nexport default Navigation\n"},"lineCount":null}},"error":null,"hash":"d2d84fb4f97adb006bd88fd30b7e938b","cacheData":{"env":{}}}