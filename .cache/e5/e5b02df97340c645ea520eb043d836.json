source":"../src/redux/user/reducer.js","original":{"line":21,"column":2}},{"generated":{"line":77,"column":0},"source":"../src/redux/user/reducer.js","original":{"line":32,"column":1}},{"generated":{"line":77,"column":1},"source":"../src/redux/user/reducer.js","original":{"line":19,"column":7}},{"name":"demoReducer","generated":{"line":80,"column":15},"source":"../src/redux/user/reducer.js","original":{"line":34,"column":15}}],"sources":{"../src/redux/user/reducer.js":"import * as constants from '../constants'\nimport jwtDecode from 'jwt-decode'\n\n// state\nlet defaultState = {\n  userId: 0,\n  username: '',\n  auth: 0,\n  email: '',\n  avatarColor: '#52c41a' // 用户头像颜色\n}\n\nif (!!localStorage.getItem('token') && localStorage.getItem('token') !== 'undefined') {\n  const { userId, username, auth, email } = jwtDecode(localStorage.token)\n  defaultState = Object.assign(defaultState, { userId, username, auth, email })\n}\n\n// reducer\nexport const demoReducer = (state = defaultState, action) => {\n  const { type, payload } = action\n  switch (type) {\n    case constants.USER_LOGIN:\n      const { userId, username, auth, email } = jwtDecode(payload.token)\n      return { ...state, userId, username, auth, email }\n\n    case constants.USER_LOGINOUT:\n      return { id: 0, username: '', auth: 0, avatarColor: '#52c41a', email: '' }\n\n    default:\n      return state\n  }\n}\n\nexport default demoReducer\n"},"lineCount":null}},"error":null,"hash":"1af4283a8cadd2dd2c0d0c6258c0f7d1","cacheData":{"env":{}}}